{
  "manifest_version": 2,

  "name": "Gnome-shell integration for Chrome",
  "short_name": "GS Chrome",
  "description": "This extension provides integration with gnome-shell and the corresponding extensions repository, codenamed SweetTooth",
  "version": "1.0.1",
  "version_name": "1.0 devel",

  "author": "Yuri Konotopov <ykonotopov@gmail.com>",
  "key": "",

  "icons": {
    "16": "icons/GnomeLogo-16.png",
    "48": "icons/GnomeLogo-48.png",
    "128": "icons/GnomeLogo-128.png"
  },

  "background": {
    "scripts": ["gs-extension.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["https://extensions.gnome.org/*"],
      "run_at": "document_start",
      "js": ["gs-content-script-start.js"]
    },
    {
      "matches": ["https://extensions.gnome.org/*"],
      "run_at": "document_end",
      "js": ["gs-content-script-end.js"]
    }
  ],
  "externally_connectable": {
      "matches": ["https://extensions.gnome.org/*"]
  },
  "permissions": [
    "nativeMessaging",
    "https://extensions.gnome.org/"
  ],
  "web_accessible_resources": ["gs-page-end.js"]
}
